class Driver {
   private String driverID;
   private String number;
   private String code;
   private String wiki;
   private String firstName;
   private String lastName;
   private String dob;
   private String nationality;
   
   Driver(String driverID, String number, String code, String wiki, String firstName, String lastName, String dob,
     String nationality) {
       this.driverID = driverID;
       this.number = number;
       this.code = code;
       this.wiki = wiki;
       this.firstName = firstName;
       this.lastName = lastName;
       this.dob = dob;
       this.nationality = nationality;
     }
}
